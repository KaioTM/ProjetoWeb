<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
  <class name="model.Usuario" table="USUARIO">
    <id name="id" column="USUARIOID"/>
    <property name="nome" column="NOME"/>
    <property name="sobrenome" column="SOBRENOME"/>
    <property name="localMoradia" column="LOCALMORADIA"/>
    <property name="esporteFavorito" column="ESPORTEFAVORITO"/>
    <property name="receberHospede" column="RECEBERHOSPEDE"/>
    <property name="quantidadeHospede" column="QUANTIDADEHOSPEDE"/>
    <property name="login" column="LOGIN"/>
    <property name="senha" column="SENHA"/>
    
    </class>
</hibernate-mapping>
